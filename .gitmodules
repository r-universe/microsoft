[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "vivainsights"]
	path = vivainsights
	url = https://github.com/microsoft/vivainsights
	shallow = true
	registered = true
[submodule "wpa"]
	path = wpa
	url = https://github.com/microsoft/wpa
	shallow = true
	registered = true
[submodule "finnts"]
	path = finnts
	url = https://github.com/microsoft/finnts
	shallow = true
	registered = true
